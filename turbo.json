{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "@mcpspec/server#build": {
      "dependsOn": ["@mcpspec/ui#build"],
      "outputs": ["dist/**", "ui-dist/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
